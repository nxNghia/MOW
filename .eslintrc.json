{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "no-const-assign":"warn",
        "no-dupe-keys":"error",
        "camelcase": [2, {"properties": "always"}],
        "eqeqeq": ["error", "always"],
        "no-trailing-spaces":"warn"
    }
}
